{
  "name": "4event-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "yarn husky install",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main/server.ts",
    "dev:debug": "cross-env NODE_ENV=development ts-node-dev --inspect=0.0.0.0:9229 --respawn --transpile-only src/main/server.ts",
    "build": "yarn rimraf dist && yarn tsc -p tsconfig-build.json",
    "start": "cross-env NODE_ENV=production node dist/main/server.js",
    "test:setup": "yarn rimraf testdb.sql && yarn migration:run:test",
    "test": "yarn test:setup && cross-env NODE_ENV=test jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "yarn test:setup && cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:unit": "yarn test:verbose --watch -c jest-unit.config.js",
    "test:integration": "yarn test:verbose --watch -c jest-integration.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "posttest": "rimraf testdb.sql",
    "before-commit": "lint-staged",
    "migration:show": "yarn ts-node-dev ./node_modules/typeorm/cli.js migration:show -d ./typeorm.ts",
    "migration:run": "yarn ts-node-dev ./node_modules/typeorm/cli.js migration:run -d ./typeorm.ts",
    "migration:prod": "cross-env NODE_ENV=production yarn typeorm migration:run -d ./typeorm.ts",
    "migration:revert": "yarn ts-node-dev ./node_modules/typeorm/cli.js migration:revert -d ./typeorm.ts",
    "migration:run:test": "cross-env NODE_ENV=test yarn ts-node-dev ./node_modules/typeorm/cli.js migration:run -d ./typeorm.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.12",
    "@types/ioredis": "^4.28.1",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "git-commit-msg-linter": "^3.2.6",
    "husky": "^7.0.1",
    "ioredis-mock": "^5.8.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "nock": "^13.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "engines": {
    "yarn": ">=1.22",
    "node": ">=14",
    "npm": ">=6"
  }
}
